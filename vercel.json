{
  "version": 2,
  "builds": [
    {
      "src": "src/sever.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
        "src": "/(.*)",
        "dest": "src/sever.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
    }
]
}